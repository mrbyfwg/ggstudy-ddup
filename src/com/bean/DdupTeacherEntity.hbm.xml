<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bean.DdupTeacherEntity" table="ddup_teacher" schema="ddupmis">
        <id name="ddupTno" column="ddup_Tno"/>
        <property name="ddupName" column="ddup_name"/>
        <property name="ddupSex" column="ddup_sex"/>
        <property name="ddupAge" column="ddup_age"/>
        <property name="ddupPhone" column="ddup_phone"/>
        <property name="ddupTitle" column="ddup_title"/>
        <set name="ddupTeachesByDdupTno" inverse="true" lazy="false">
            <key>
                <column name="ddup_Tno"/>
            </key>
            <one-to-many not-found="ignore" class="com.bean.DdupTeachEntity" />
        </set>
    </class>
</hibernate-mapping>